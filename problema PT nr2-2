#include <bits/stdc++.h>
using namespace std;

struct asdf{
    int data;
    asdf *next;
};

void insertt(asdf *head, int info)
{
    asdf *current= head;
    while(current->next!=NULL && current->next->data < info)
        current=current->next;
    asdf *newnode=new asdf;
    newnode->data=info;
    newnode->next=current->next;
    current->next=newnode;
}

int main()
{
    asdf *head;
    head = new asdf;
    head->next=NULL;
    int l1;
    cin>>l1;
    for(int i=0;i<l1;++i)
    {
        int x;
        cin>>x;
        insertt(head,x);
    }
    asdf *head2=new asdf;
    head2->next=NULL;
    int l2;
    cin>>l2;
    for(int i=0;i<l2;++i)
    {
        int x;
        cin>>x;
        insertt(head2,x);
    }

    int v1[100],v2[100];
    int i1=0;
    int i2=0;
    asdf *c1,*c2;
    c1=head;
    c2=head2;
    while(c1->next!=NULL && c2->next!=NULL)
    {
        if(c1->next->data == c2->next->data)
        {
            v1[i1++]=c1->next->data;
            v2[i2++]=c1->next->data;
            c1=c1->next;
            c2=c2->next;
        }
        else if(c1->next->data > c2->next->data)
        {
            v2[i2++]=c2->next->data;
            c2=c2->next;
        }
        else
        {
            v2[i2++]=c1->next->data;
            c1=c1->next;
        }
    }
    if(c1->next!=NULL)
        while(c1->next!=NULL)
        {
            v2[i2++]=c1->next->data;
            c1=c1->next;
        }
    if(c2->next!=NULL)
        while(c2->next!=NULL)
        {
            v2[i2++]=c2->next->data;
            c2=c2->next;
        }
    for(int i=0;i<i1;++i)
        cout<<v1[i]<<" ";
    cout<<'\n';
    for(int i=0;i<i2;++i)
        cout<<v2[i]<<" ";


    return 0;
}
